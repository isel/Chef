var __BASE = __JSPR_ROOT + \"ver<%= rev %>/build/\";
var __FILTER = "";
var __SERVICE_HOST = \"http://<%= @host %>\";
var __AUTHENTICATION_URI = \"http://<%= @host %>:81/httpIssue.svc/Authenticate/?realm=http://localhost/RP&type=SAML&tenantid=VGC\";